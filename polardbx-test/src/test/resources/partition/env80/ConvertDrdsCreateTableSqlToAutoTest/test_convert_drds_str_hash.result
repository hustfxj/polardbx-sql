create table `tb3` (
`col1` varchar(11) not null,
`col2` varchar(11) not null,
primary key(`col1`)
) dbpartition by str_hash(`col2`, 0, 1) tbpartition by str_hash(`col1`, -1, 5) tbpartitions 2;
